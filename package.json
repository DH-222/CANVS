{
  "name": "contributor-village",
  "description": "Dev for Contributor Village",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahat/hackathon-starter.git"
  },
  "author": "Omar Scott",
  "license": "MIT",
  "scripts": {
    "start-dev": "PLACE=DEV nodemon app.js",
    "start-prod": "PLACE=PROD nodemon app.js",
    "start": "PLACE=PROD node app.js",
    "test": "mocha --reporter spec",
    "css": "npm rebuild node-sass",
    "eslint": "eslint **/*.js",
    "sass": "node-sass --include-path scss public/css/main.scss public/css/main.css",
    "wsass": "nodemon -e scss -x \"npm run sass\""
  },
  "dependencies": {
    "aws-sdk": "^2.393.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "clockwork": "^0.1.4",
    "co": "^4.6.0",
    "compression": "^1.7.3",
    "connect-mongo": "^1.3.2",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.4",
    "express-flash": "^0.0.2",
    "express-rate-limit": "^3.3.2",
    "express-session": "^1.15.6",
    "express-status-monitor": "^1.2.3",
    "express-validator": "^5.0",
    "fbgraph": "^1.4.4",
    "gm": "^1.23.1",
    "helmet": "^3.15.0",
    "imagemagick": "^0.1.3",
    "instagram-node": "^0.5.8",
    "lastfm": "^0.9.3",
    "lob": "^4.1.1",
    "lusca": "^1.6.1",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.13.18",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multiparty": "^4.2.1",
    "mysql": "^2.15.0",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^4.7.0",
    "nodemon": "^1.18.9",
    "passport": "0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "stripe": "^4.25.0",
    "tumblr.js": "^1.1.1",
    "twilio": "^3.27.1",
    "twit": "^2.2.11",
    "uuid": "^3.3.2",
    "validator": "^8.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.15.0",
    "zipcodes": "^7.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
