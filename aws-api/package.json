{
  "name": "hackathon-starter",
  "version": "4.3.0",
  "description": "A boilerplate for Node.js web applications",
  "author": "Omar Scott",
  "license": "MIT",
  "scripts": {
    "start-dev": "PLACE=DEV nodemon app.js",
    "start-prod": "PLACE=PROD nodemon app.js",
    "start": "PLACE=PROD node app.js",
    "test": "mocha --reporter spec",
    "eslint": "eslint **/*.js"
  },
  "dependencies": {
    "aws-sdk": "^2.395.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.17.2",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "clockwork": "^0.1.4",
    "co": "^4.6.0",
    "compression": "^1.7.0",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.4",
    "express-flash": "^0.0.2",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^3.4.0",
    "express-session": "^1.15.5",
    "express-status-monitor": "^1.0.0",
    "express-validator": "^5.3.1",
    "fbgraph": "^1.4.1",
    "fs": "0.0.1-security",
    "gm": "^1.23.1",
    "instagram-node": "^0.5.8",
    "jsonwebtoken": "^8.4.0",
    "lastfm": "^0.9.3",
    "lob": "^4.1.1",
    "lusca": "^1.5.1",
    "mkdir": "0.0.2",
    "mongoose": "^4.13.18",
    "morgan": "^1.8.2",
    "multer": "^1.4.0",
    "nodemailer": "^4.7.0",
    "nodemon": "^1.18.9",
    "passport": "0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "path": "^0.12.7",
    "paypal-rest-sdk": "^1.7.1",
    "request": "^2.81.0",
    "rimraf": "^2.6.3",
    "stripe": "^4.24.0",
    "validator": "^8.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-import": "^2.15.0",
    "supertest": "^3.4.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
